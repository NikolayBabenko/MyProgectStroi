<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="274dp"
        android:layout_marginTop="29dp"
        android:text="@string/mp75"
        tools:layout_editor_absoluteX="213dp"
        tools:layout_editor_absoluteY="16dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="79dp"
        android:text="@string/Crisel"
        tools:layout_editor_absoluteX="306dp"
        tools:layout_editor_absoluteY="16dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView6"
        android:layout_alignBottom="@+id/mp75"
        android:layout_marginStart="42dp"
        android:layout_marginBottom="6dp"
        android:text="@string/squareMeters"
        tools:layout_editor_absoluteX="4dp"
        tools:layout_editor_absoluteY="70dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="65dp"
        android:layout_marginTop="194dp"
        android:text="@string/bags"
        tools:layout_editor_absoluteX="4dp"
        tools:layout_editor_absoluteY="124dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/vsego"
        android:layout_alignStart="@+id/textView3"
        android:text="@string/priseForAll"
        tools:layout_editor_absoluteX="4dp"
        tools:layout_editor_absoluteY="218dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="12dp"
        android:layout_marginBottom="28dp"
        android:text="@string/priseAll"
        tools:layout_editor_absoluteX="4dp"
        tools:layout_editor_absoluteY="302dp" />

    <EditText
        android:id="@+id/mp75"
        android:layout_width="72dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/res_mp75"
        android:layout_alignParentTop="true"
        android:layout_marginStart="-1dp"
        android:layout_marginTop="61dp"
        android:background="@android:color/black"
        android:ems="10"
        android:inputType="numberDecimal"
        android:textColor="@color/colorPrimaryDark"
        tools:layout_editor_absoluteX="180dp"
        tools:layout_editor_absoluteY="55dp" />

    <EditText
        android:id="@+id/crazer"
        android:layout_width="77dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/mp75"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="56dp"
        android:ems="10"
        android:inputType="numberDecimal"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="289dp"
        tools:layout_editor_absoluteY="55dp"
        android:background="@android:color/black"
        android:textColor="@color/colorPrimaryDark"/>

    <TextView
        android:id="@+id/res_mp75"
        android:layout_width="82dp"
        android:layout_height="36dp"
        android:layout_alignTop="@+id/textView4"
        android:layout_centerHorizontal="true"
        tools:layout_editor_absoluteX="180dp"
        tools:layout_editor_absoluteY="124dp" />

    <TextView
        android:id="@+id/res_craser"
        android:layout_width="65dp"
        android:layout_height="38dp"
        android:layout_alignStart="@+id/textView2"
        android:layout_alignTop="@+id/textView4"
        android:layout_marginStart="-14dp"
        android:layout_marginTop="1dp"
        tools:layout_editor_absoluteX="296dp"
        tools:layout_editor_absoluteY="122dp" />

    <EditText
        android:id="@+id/vsego"
        android:layout_width="105dp"
        android:layout_height="45dp"
        android:layout_alignEnd="@+id/itogo"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="-94dp"
        android:layout_marginBottom="145dp"
        android:background="@android:color/black"
        android:inputType="numberDecimal"
        android:textColor="@android:color/holo_green_dark"
        tools:layout_editor_absoluteX="140dp"
        tools:layout_editor_absoluteY="208dp" />

    <TextView
        android:id="@+id/itogo"
        android:layout_width="122dp"
        android:layout_height="59dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="237dp"
        android:layout_marginBottom="11dp"
        android:textSize="42dp"
        tools:layout_editor_absoluteX="180dp"
        tools:layout_editor_absoluteY="309dp" />

    <Button
        android:id="@+id/start"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView6"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="-381dp"

        android:layout_marginBottom="6dp"
        android:text="@string/btnStart"
        tools:layout_editor_absoluteX="52dp"
        tools:layout_editor_absoluteY="368dp" />

</RelativeLayout>